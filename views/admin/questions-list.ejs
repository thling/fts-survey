<% questions.forEach(function (q) { %>
    <!-- Generate questions cards for each question -->
    <div class="col l4 m6 s12">
        <div class="card">
            <div class="card-content">
                <span class="card-title"><%- q.title %></span>
                <p><%- q.description %></p>
            </div>
        </div>
    </div>
<% }); %>
